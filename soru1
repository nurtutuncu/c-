#include <stdio.h>
#include <stdlib.h>
int a, b;
int main()
{
    printf("Birinci sayiyi giriniz: ");
    scanf("%d", &a);
    printf("Ikinci sayiyi giriniz: ");
    scanf("%d", &b);

    printf("Toplam: %d\n", a + b);
    printf("Fark: %d\n", a - b);
    printf("Carpim: %d\n", a * b);
    printf("Bolum: %d\n", a / b);
    return 0;
}
