#include <stdio.h>
#include <stdlib.h>

char harf;
int main()
{
    printf(" harf giriniz: ");
    scanf(" %c", & harf);

    if (harf=='a' || harf=='e' || harf=='i' || harf=='o' || harf=='u' ||
        harf=='A' || harf=='E' || harf=='I' || harf=='O' || harf=='U')
        {
        printf("Sesli harf\n");
    }
    else
    {
        printf("Sessiz harf\n");
    }
    return 0;
}
